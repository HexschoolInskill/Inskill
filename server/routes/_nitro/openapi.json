    "schemas": {
      "sign_up_Request": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          },
          "password": {
            "type": "string",
            "minLength": 8,
            "maxLength": 30
          },
          "confirmPassword": {
            "type": "string",
            "minLength": 8,
            "maxLength": 30
          },
          "username": {
            "type": "string"
          }
        },
        "required": ["email", "password", "confirmPassword", "username"],
        "example": {
          "email": "user@example.com",
          "password": "password123",
          "confirmPassword": "password123",
          "username": "user123"
        }
      }
    }
  },
